<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.DetailFragment">

    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/viewPager"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/Name"
        android:layout_width="70dp"
        android:layout_height="34dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewPager" />

    <TextView
        android:id="@+id/averagerating"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name" />

    <TextView
        android:id="@+id/avgratingnumb"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/averagerating"
        app:layout_constraintTop_toTopOf="@+id/averagerating" />

    <ImageView
        android:id="@+id/avgrateimg"
        android:layout_width="142dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/avgratingnumb"
        app:layout_constraintTop_toTopOf="@+id/avgratingnumb"
        app:srcCompat="@drawable/otwarte" />

    <TextView
        android:id="@+id/details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/avgrateimg"
        app:layout_constraintTop_toBottomOf="@+id/avgrateimg" />

    <TextView
        android:id="@+id/googlerating"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="108dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name" />

    <TextView
        android:id="@+id/goognumb"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/googlerating"
        app:layout_constraintTop_toTopOf="@+id/googlerating" />

    <ImageView
        android:id="@+id/googimg"
        android:layout_width="142dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/goognumb"
        app:layout_constraintTop_toTopOf="@+id/goognumb"
        app:srcCompat="@drawable/otwarte" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="53dp"
        android:layout_height="31dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/Name"
        app:layout_constraintTop_toBottomOf="@+id/viewPager"
        app:srcCompat="@drawable/otwarte" />

    <TextView
        android:id="@+id/open"
        android:layout_width="83dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:text="czy owarte?"
        app:layout_constraintStart_toEndOf="@+id/Name"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <TextView
        android:id="@+id/price"
        android:layout_width="65dp"
        android:layout_height="27dp"
        android:layout_marginStart="44dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <TextView
        android:id="@+id/triperating"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="176dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name" />

    <TextView
        android:id="@+id/tripnumb"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/triperating"
        app:layout_constraintTop_toTopOf="@+id/triperating" />

    <ImageView
        android:id="@+id/tripimg"
        android:layout_width="142dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/tripnumb"
        app:layout_constraintTop_toTopOf="@+id/tripnumb"
        app:srcCompat="@drawable/otwarte" />

    <TextView
        android:id="@+id/fbrating"
        android:layout_width="62dp"
        android:layout_height="25dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="240dp"
        android:visibility="gone"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Name" />

    <TextView
        android:id="@+id/fbnumb"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:visibility="gone"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/fbrating"
        app:layout_constraintTop_toTopOf="@+id/fbrating" />

    <ImageView
        android:id="@+id/fbimg"
        android:layout_width="142dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/fbnumb"
        app:layout_constraintTop_toTopOf="@+id/fbnumb"
        app:srcCompat="@drawable/otwarte" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="104dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fbrating" />


</androidx.constraintlayout.widget.ConstraintLayout>